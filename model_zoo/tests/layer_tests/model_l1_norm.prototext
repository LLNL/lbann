model {
  data_layout: "data_parallel"
  mini_batch_size: 11
  block_size: 256
  num_epochs: 0
  num_parallel_readers: 0
  procs_per_model: 0

  ###################################################
  # Objective function
  ###################################################

  objective_function {
    layer_term { layer: "l2" }
  }

  ###################################################
  # Callbacks
  ###################################################
  callback { print {} }
  callback { timer {} }
  callback {
    gradient_check {
      verbose: false
      fail_on_error: true
    }
  }

  ###################################################
  # Layers
  ###################################################

  layer {
    name: "data"
    data_layout: "data_parallel"
    input {
      io_buffer: "partitioned"
    }
  }

  # Input data
  layer {
    name: "x"
    weights_layer {
      dims: "5"
    }
    data_layout: "model_parallel"
    weights: "x_vals"
  }
  weights {
    name: "x_vals"
    value_initializer {
      values: "1 -0.5 0.25 -0.125 0.125"
    }
  }

  # Variations of L1 norm layer
  layer {
    parents: "x"
    name: "l1_norm_model_parallel"
    l1_norm {}
    data_layout: "model_parallel"
  }
  layer {
    parents: "x"
    name: "l1_norm_data_parallel"
    l1_norm {}
    data_layout: "data_parallel"
  }

  # Combine into objective function
  layer {
    parents: "l1_norm_model_parallel l1_norm_data_parallel"
    name: "sum"
    sum {}
  }
  layer {
    parents: "sum"
    name: "l2"
    l2_norm2 {}
  }

}
